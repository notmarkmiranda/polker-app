<nav>
  <% if current_user %>
    <div class="d-flex">
      <%= button_to "Sign Out", sign_out_path, method: :delete, class: "btn btn-outline-danger" %>
    </div>
  <% else %>
    <div>
      <%= link_to "Sign In", sign_in_path, class: "btn btn-outline-primary" %>&nbsp;
      <%= link_to "Sign Up", sign_up_path, class: "btn btn-outline-info" %>
    </div>
  <% end %>
</nav>
